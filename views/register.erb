<form method="POST" action="/register">
	<input type="text" name="[post]user_name" placeholder="Username"><br>
	<input type="text" name="[post]first_name" placeholder="First Name"><br>
	<input type="text" name="[post]last_name" placeholder="Last Name"><br>
	<input type="text" name="[post]email" placeholder="Email"><br>
	<input type="password" name="[post]password" placeholder="Password"><br>
	<input type="password" name="confirm_password" placeholder="Confirm Password"><br>
	<input type="submit">
</form>
<p>This form needs to be able to check the input</p>
